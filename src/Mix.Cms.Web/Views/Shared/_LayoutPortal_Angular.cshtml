@using Microsoft.AspNetCore.Mvc.Localization
@inject UserManager<ApplicationUser> UserManager
<!doctype html>
<html lang="en">

<head>
    <base href="/portal">
    <title>@ViewData["Title"] - Angular mixcore</title>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=1920">
    <meta data-react-helmet="true" name="viewport" content="width=device-width, initial-scale=1, viewport-fit=cover, user-scalable=no">
    <link rel="icon" href="/css/portal/img/mixcore-logo-blue.png">
    @*<link href="~/css/shared.min.css" rel="stylesheet" />*@
    <link rel="stylesheet" href="~/css/app-vendor.min.css" />

    <link rel="stylesheet" href="~/css/portal/portal.white-sd.css" />
    <link rel="stylesheet" href="~/css/portal.min.css" />
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.6.1/css/all.css" integrity="sha384-gfdkjb5BdAXd+lj+gudLWI+BXq4IuLW5IT+brZEZsLFm++aCMlF1V92rMkPaX4PP" crossorigin="anonymous">

    @RenderSection("Styles", required: false)
</head>

<body>
    <script>
        window.fbAsyncInit = function () {
            FB.init({
                appId: '@(MixService.GetConfig<string>("FacebookAppId", ViewBag.culture))',
                autoLogAppEvents: true,
                xfbml: true,
                version: 'v3.2'
            });
        };

        (function (d, s, id) {
            var js, fjs = d.getElementsByTagName(s)[0];
            if (d.getElementById(id)) { return; }
            js = d.createElement(s); js.id = id;
            js.src = "https://connect.facebook.net/en_US/sdk.js";
            fjs.parentNode.insertBefore(js, fjs);
        }(document, 'script', 'facebook-jssdk'));
    </script>

    @RenderBody()
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.11.0/umd/popper.min.js" integrity="sha384-b/U6ypiBEHpOf/4+1nzFpr53nxSS+GLCkfwBdFNTxtclqqenISfwAzpKaMNFNmj4" crossorigin="anonymous"></script>
    <script src="~/js/framework.min.js"></script>
    <script src="~/js/bootstrap.min.js"></script>
    <script src="~/js/shared.min.js"></script>
    <script src="~/js/portal.min.js"></script>

    <script src="~/js/app-portal.min.js"></script>
    <script src="~/js/app-shared.min.js"></script>

    <!-- End Components -->
    @RenderSection("Scripts", required: false)

    <!--Datatables-->
    @*@RenderSection("Modals", required: false)*@
    @*@await Component.InvokeAsync(typeof(Mix.Cms.Mvc.Areas.Portal.ViewComponents.Modals))*@
</body>
</html>